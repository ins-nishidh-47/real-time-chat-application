{
    "version": 2,
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "functions": {
      "server/**/*.js": {
        "runtime": "@vercel/node"
      }
    },
    "routes": [
      { "src": "/server/(.*)", "dest": "/server/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  